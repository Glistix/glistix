let
  inherit (builtins.import ./../gleam.nix) byteSize intFromBitSlice;
  
  go =
    x:
      let
        _assert' =
          if byteSize x != 3
          then builtins.throw "Assignment pattern did not match"
          else null;
        a = builtins.seq _assert' (intFromBitSlice x 0 2);
        b = builtins.seq _assert' (intFromBitSlice x 2 3);
      in builtins.seq _assert' [ a b ];
