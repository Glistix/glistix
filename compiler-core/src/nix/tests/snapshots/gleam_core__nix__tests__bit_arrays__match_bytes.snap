let
  inherit (builtins.import ./../gleam.nix) byteSize byteAt;
  
  go =
    x:
      let
        _assert' =
          if (byteAt x 0) != 1 || byteSize x != 2
          then builtins.throw "Assignment pattern did not match"
          else null;
        y = builtins.seq _assert' (byteAt x 1);
      in builtins.seq _assert' y;
